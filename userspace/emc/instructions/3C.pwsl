linkdef/"emc3C","emc60","global"
{
	// Print kernel message.
	// 3C/[process name],[color],[message]
	getitem/"argument1",1/"in1"
	getitem/"argument1",2/"in2"
	getitem/"argument1",3/"in3"
	set/"msg",in1," (pid ",pid,")"
	kmsg/msg,in2,in3
}
